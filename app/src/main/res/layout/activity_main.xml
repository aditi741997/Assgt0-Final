<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="nikhil.ayush.aditi.assgt0_final1.MainActivity"
    tools:showIn="@layout/activity_main"
    android:background="#c3eb79">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="COP 290"
        android:id="@+id/cop"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Team Registration"
        android:id="@+id/treg"
        android:layout_below="@+id/cop"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Name 1"
        android:id="@+id/name1"
        android:layout_marginTop="32dp"
        android:layout_below="@+id/treg"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Entry No. 1"
        android:id="@+id/entry1"
        android:layout_below="@+id/name1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="8dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Name 2"
        android:id="@+id/name2"
        android:layout_below="@+id/entry1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Entry No. 2"
        android:id="@+id/entry2"
        android:layout_below="@+id/name2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="8dp"/>


    <!--         android:layout_above="@+id/treg"
        android:layout_alignRight="@+id/editText2"
        android:layout_alignEnd="@+id/editText2"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2"-->

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Entry1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
            android:layout_below="@+id/treg"
        android:layout_toRightOf="@+id/entry1"
        android:layout_toEndOf="@+id/entry1"
        android:layout_marginTop="47dp"
        android:layout_marginLeft="15dp"
        />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Name1"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/treg"
        android:layout_alignStart="@+id/Entry1"
        android:layout_alignLeft="@+id/Entry1"
        android:layout_alignEnd="@+id/Entry1"
        android:layout_alignRight="@+id/Entry1"
        />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Name2"
        android:layout_below="@+id/treg"
        android:layout_alignStart="@+id/Entry1"
        android:layout_alignLeft="@+id/Entry1"
        android:layout_alignEnd="@+id/Entry1"
        android:layout_alignRight="@+id/Entry1"
        android:layout_marginTop="97dp"/>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Entry2"
        android:layout_below="@+id/treg"
        android:layout_alignStart="@+id/Entry1"
        android:layout_alignLeft="@+id/Entry1"
        android:layout_alignEnd="@+id/Entry1"
        android:layout_alignRight="@+id/Entry1"
        android:layout_marginTop="129dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Name3"
        android:layout_below="@+id/treg"
        android:layout_alignStart="@+id/Entry1"
        android:layout_alignLeft="@+id/Entry1"
        android:layout_alignEnd="@+id/Entry1"
        android:layout_alignRight="@+id/Entry1"
        android:layout_marginTop="179dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Entry3"
        android:layout_below="@+id/treg"
        android:layout_alignStart="@+id/Entry1"
        android:layout_alignLeft="@+id/Entry1"
        android:layout_alignEnd="@+id/Entry1"
        android:layout_alignRight="@+id/Entry1"
        android:layout_marginTop="211dp"
 />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Name 3"
        android:id="@+id/name3"
        android:layout_below="@+id/entry2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Entry No. 3"
        android:id="@+id/entry3"
        android:layout_below="@+id/name3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="8dp"/>





    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register the team"
        android:id="@+id/submit"
        android:layout_below="@+id/entry3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:onClick="SendData"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Message"
        android:id="@+id/msg"
        android:layout_below="@+id/entry3"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="120dp"
 />
</RelativeLayout>
</ScrollView>
