<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="100dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#c3eb79"
        android:startColor="#456423"
        android:endColor="#C8EBC8"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="3dp"
        />
    <size
        android:width="45dp"
        android:height="45dp"
        />
    <stroke
        android:width="2dp"
        android:color="#C20202"
        />
</shape>