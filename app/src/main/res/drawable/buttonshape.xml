<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="100dp"
        />
    <gradient
        android:angle="45"
        android:centerX="50%"
        android:centerColor="#B5FFAB"
        android:startColor="#B5FFAB"
        android:endColor="#B5FFAB"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="-5dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:width="45dp"
        android:height="45dp"
        />
    <stroke
        android:width="2dp"
        android:color="#5FA346"
        />
</shape>